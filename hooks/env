[ -n "$SOURCE_BRANCH" ]  || SOURCE_BRANCH=$(git symbolic-ref -q --short HEAD)
[ -n "$VCS_REF" ]       || VCS_REF=$(git rev-parse -q --short HEAD)
 
if [[ "${SOURCE_BRANCH/-*/}" =~ ^[0-9][0-9.]*$ ]]; then
	VERSION=${SOURCE_BRANCH/-*/}
fi

if [[ "${VERSION}" == "3.13" ]]; then
	EXTRA_TAG="-t alpine-postfix:latest"
fi
 
[ -n "$IMAGE_NAME" ] || IMAGE_NAME=alpine-postfix:${SOURCE_BRANCH}
